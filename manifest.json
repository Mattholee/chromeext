{
    "name": "ChatGPT teaches you how to highlight text",
    "description": "Extension that summarizes highlighted text via ChatGPT",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "js": ["content.js"],
          "matches": [
            "http://*/*",
            "https://*/*"
          ],
          "run_at": "document_end"
        }
      ]
  }